{
	"name": "@softstack/typed-stringify",
	"version": "1.0.0",
	"description": "Stringifies and parses objects while restoring the proper type on parse",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/chainsulting/typed-stringify"
	},
	"scripts": {
		"test": "jest --config jestconfig.json",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"keywords": [
		"stringify"
	],
	"author": "Softstack <hello@softstack.de> (https://softstack.de)",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/lodash": "^4.14.180",
		"@typescript-eslint/eslint-plugin": "^5.15.0",
		"@typescript-eslint/parser": "^5.15.0",
		"bignumber.js": "^9.0.2",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"jest": "^27.5.1",
		"lodash": "^4.17.21",
		"prettier": "^2.6.0",
		"ts-jest": "^27.1.3",
		"typescript": "^4.6.2"
	},
	"files": [
		"lib/**/*"
	]
}
